<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@floor) do |f| %>
      <%= render 'shared/floor_error_messages' %>

      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control' %>
      
      <%= f.file_field :picture %>

      <%= f.submit "Create the floor", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>